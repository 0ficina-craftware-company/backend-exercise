name: dockercompose512312001612867103
services:
  mydotnettestapi:
    build:
      context: C:\Users\Rui\Documents\tests\coolCarsApi
      dockerfile: myDotNetTestApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: myDotNetTestApi
      target: BASE
    container_name: myDotNetTestApi
    depends_on:
      slqserver-express:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: mydotnettestapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/myDotNetTestApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\Rui\Documents\tests\coolCarsApi\myDotNetTestApi
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rui\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rui\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rui\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rui\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rui\Documents\tests\coolCarsApi
      target: /src
      bind:
        create_host_path: true
  slqserver-express:
    container_name: slqserver-express
    environment:
      ACCEPT_EULA: "true"
      MSSQL_PID: Express
      SA_PASSWORD: yourStrong(!)Password
    image: docker pull mcr.microsoft.com/mssql/server:2017-CU30-ubuntu-18.04
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
networks:
  default:
    name: dockercompose512312001612867103_default